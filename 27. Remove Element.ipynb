   "source": [
    "class Solution:\n",
    "    def removeElement(self, nums: List[int], val: int) -> int:\n",
    "        l = 0\n",
    "        for r in range(0, len(nums)):\n",
    "            if nums[r] != val:\n",
    "                nums[l] = nums[r]\n",
    "                l += 1\n",
    "        return l"
   ]
  }
