   "source": [
    "class Solution:\n",
    "    def strStr(self, haystack: str, needle: str) -> int:\n",
    "        if needle == \"\":\n",
    "            return 0\n",
    "        for i in range(len(haystack) + 1 - len(needle)):\n",
    "            if haystack[i: i+len(needle)] == needle:\n",
    "                return i\n",
    "        return -1"
   ]
  }
